    "test": "flutter test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/peemkay-softtech/portfolio"
  },
  "keywords": [
    "flutter",
    "web",
    "portfolio",
    "software-development",
    "peemkay-softtech"
  ],
  "engines": {
    "node": ">=14.0.0",
    "python": ">=3.6.0"
  },
  "config": {
    "ports": {
      "production": 3000,
      "development": 3001
    }
  }
}
